library(jsonlite)

# Load the tidyr package
library(tidyr)
library(dplyr)


#Verritree
verritree1 <- read.csv("verritree1.csv")
verritree2 <- read.csv("verritree2.csv")
verritree3 <- read.csv("verritree3.csv")

#Reforestum
reforestum <- read.csv("reforestum.csv")


#reforestum

reforestum_json_file_path <- "reforestum.json"

# Read the JSON file into a dataframe
reforestum <- fromJSON(reforestum_json_file_path)




#Atlas
atlas <- read.csv2("Atlas_data_frame.csv")

#Explorerland

explorer_json_file_path <- "explorer_land.json"

# Read the JSON file into a dataframe
explorerland <- fromJSON(explorer_json_file_path)

# Unnest the geometry and properties columns
explorerland <- explorerland %>%
  unnest_wider(geometry, names_sep = "_geometry_") %>%
  unnest_wider(properties, names_sep = "_properties_")


#Forest_trends
forest_trends <- read.csv("forest_trends.csv")

#reforest_action
reforest_action <- read.csv2("reforest_action.csv")

#restor_eco

restorr_json_file_path <- "restor_eco.json"

# Read the JSON file into a dataframe
restor_eco <- fromJSON(restorr_json_file_path)

#treenation

treenation_json_file_path <- "treenation.json"

# Read the JSON file into a dataframe
treenation <- fromJSON(treenation_json_file_path)



#Columnnames 
atlas_names <- names(atlas)
print(atlas_names)


# Assuming df is your dataframe
explorer_column_names <- colnames(explorerland)
print(explorer_column_names)


# Assuming df is your dataframe
forest_trends_column_names <- colnames(forest_trends)
print(forest_trends_column_names)


# Assuming df is your dataframe
reforest_action_column_names <- colnames(reforest_action)
print(reforest_action_column_names)


# Assuming df is your dataframe
restor_eco_column_names <- colnames(restor_eco)
print(restor_eco_column_names)

# Assuming df is your dataframe
treenation_column_names <- colnames(treenation)
print(treenation_column_names)

#Reforestum
reforestum_column_names <- colnames(reforestum)
print(reforestum_column_names)
